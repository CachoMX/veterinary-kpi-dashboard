# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI API (if using AI features)
OPENAI_API_KEY=your_openai_api_key

# Sync Security
SYNC_SECRET_KEY=your-sync-secret-key

# Google Analytics 4 Configuration
# This is the complete service account JSON credentials as a single-line string
# Get this from Google Cloud Console > IAM & Admin > Service Accounts
# Download the JSON key file and convert to a single line (remove newlines)
GA4_SERVICE_ACCOUNT_CREDENTIALS={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"...@....iam.gserviceaccount.com","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"..."}

# Optional: Separate secret key for GA4 sync endpoints
GA4_SYNC_SECRET=your-ga4-sync-secret-key

# Vercel Cron Job Secret (required for cron authentication)
# Generate a random secret: openssl rand -base64 32
CRON_SECRET=your-cron-secret-key

# Monday.com Configuration (existing)
MONDAY_TOKEN=your_monday_api_token
